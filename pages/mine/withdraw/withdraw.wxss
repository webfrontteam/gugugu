/* pages/mine/withdraw/withdraw.wxss */

.weui-panel{
  margin-top:200px;
  margin-left: 15px;
  margin-right: 15px
}

.weui-panel__hd1{padding:14px 15px 20px;color:#000000;font-size:15px;top:5px;position:relative;}
.weui-panel__hd1:after{content:" ";position:absolute;left:0;bottom:0;right:0;height:1px;
}

.weui-media-box__hd_in-appmsg{margin-right:20px;width:120px;height:60px;line-height:60px;text-align:right}

.withdraw_userimage{
  /* margin-left: 15px; */
  margin-top: 10px;
  width: 30px;
  height: 30px;
  border-radius: 100px;
  text-align:center;
  vertical-align:top
  }

.view-flex-row0{
  display: flex;
  flex-direction: row;
  background-color:#f8f6f6
}

.view-flex-row{
  display: flex;
  flex-direction: row
}

.weui-media-box__title2{
  font-weight:400;
  font-size:17px;
  width:auto;
  overflow:hidden;
  text-overflow:ellipsis;
  margin-top: 5px;
  white-space:nowrap;
  word-break:break-all
}

.weui-media-box__money{
  color:#000000;
  font-size:30px;
  line-height:1.2;
  overflow:hidden;
  text-overflow:ellipsis;
  margin-left: 12px;
  margin-top: 2px;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2
}

.weui-btn{
  top:12px
}